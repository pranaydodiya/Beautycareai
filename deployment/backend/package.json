{
  "name": "quirky",
  "version": "1.0.0",
  "description": "Ecommerce shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix frontend",
    "python-service": "cd python_service && python start_python_service.py",
    "python-deepface": "cd python_service && python start_deepface_service.py",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run python-deepface\"",
    "dev-backend": "concurrently \"npm run server\" \"npm run python-deepface\"",
    "data:destroy": "node backend/seeder -d",
    "data:import": "node backend/seeder "
  },
  "author": "Tanya Malik",
  "license": "MIT",
  "dependencies": {
    "@clerk/backend": "^2.14.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "uuid": "^13.0.0"
  }
}
